{
    "compilerOptions": {
      "target": "ES2015",
      "module": "CommonJS",
      "moduleResolution": "node",
      "lib": [
        "ES2020",
      ],
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "strictBindCallApply": true,
      "strictPropertyInitialization": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "esModuleInterop": true,
      "types": ["node"]
    },
  }